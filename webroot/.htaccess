RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_URI} !index\.php [NC]
RewriteCond %{REQUEST_URI} !phpinfo\.php [NC]
RewriteCond %{REQUEST_URI} !check\.php [NC]
RewriteRule ^([^/]+)(.*)?$ index.php?path=$1$2   [L]

